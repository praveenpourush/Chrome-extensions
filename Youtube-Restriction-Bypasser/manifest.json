{
  "manifest_version": 2,

  "name": "Youtube Restriction bypasser!",
  "description": "Opens restricted youtube videos!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
